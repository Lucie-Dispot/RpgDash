<p id="notice"><%= notice %></p>

<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <th>File</th>
      <th>Source</th>
      <th>Author</th>
      <th>Duration</th>
      <th>Loop</th>
      <th>Attr stress</th>
      <th>Attr epicness</th>
      <th>Attr happiness</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.file %></td>
        <td><%= song.source %></td>
        <td><%= song.author %></td>
        <td><%= song.duration %></td>
        <td><%= song.loop %></td>
        <td><%= song.attr_stress %></td>
        <td><%= song.attr_epicness %></td>
        <td><%= song.attr_happiness %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
